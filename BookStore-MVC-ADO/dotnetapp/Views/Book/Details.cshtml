@model dotnetapp.Models.Book

<h2>Book Details</h2>

<div>
    <dl class="dl-horizontal">
        <dt>Id:</dt>
        <dd>@Model.BookID</dd>
        <dt>Title:</dt>
        <dd>@Model.Title</dd>
        <dt>Author:</dt>
        <dd>@Model.Author</dd>
        <dt>Genre:</dt>
        <dd>@Model.Genre</dd>
        <dt>Price:</dt>
        <dd>@Model.Price</dd>
        <dt>Quantity:</dt>
        <dd>@Model.Quantity</dd>
    </dl>
</div>
<p>
    @Html.ActionLink("Back to List", "Index")
</p>
